1 ]=> (define (sqr x)
  	(* x x))
;Value: sqr

1 ]=> (define (cube x)
  	(* x x x))
;Value: cube

1 ]=> (define (improve x guess)
  	(/ (+ (/ x (sqr guess))
		  (* 2 guess))
	   3))
;Value: improve

1 ]=> (define (good-enough? x guess)
  	(< (abs (- (cube guess) x)) 0.00001))
;Value: good-enough?

1 ]=> (define (cube-root x guess)
	(if (good-enough? x guess)
	  	guess
		(cube-root x (improve x guess))
	)
)
;Value: cube-root

1 ]=> (cube-root 27 1)
;Value: 

